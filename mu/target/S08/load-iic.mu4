( This file loads the IIC over serial or USB code into the flash of an S08
  part.)

ld! target/S08/build.mu4  ( sets radix to hex!)

.ifdef in-ram
   ram
.else
   @iic  flash region!
.then

.reg EPCTL0 .if
ld target/S08/iic-usb-core.mu4
.else
ld target/S08/iic-serial-core.mu4
.then
